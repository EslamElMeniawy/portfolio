{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@packageJson": ["./package.json"],
      "@appJson": ["./app.json"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
